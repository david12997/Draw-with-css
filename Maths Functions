        "### Funcion cuadratica"
      ],
      "metadata": {
        "id": "ub80Tp855-Au"
      }
    },
    {
      "cell_type": "code",
      "source": [
        "#funcuion cuadratica y= x´2 \n",
        "\n",
        "def func(x):\n",
        "  return x**2\n",
        "\n",
        "def func_h(x):\n",
        "  return x**3\n",
        "\n",
        "def func_g(x,move_x,move_y):\n",
        "  return((x-move_x)**2 + move_y)\n",
        "\n",
        "resolution = 1000\n",
        "x = np.linspace(-10.0,10.0,num=resolution)\n",
        "y = func(x)\n",
        "g = func_g(x,4,20)\n",
        "h = func_h(x)\n",
        "\n",
        "fig, ax = plt.subplots()\n",
        "\n",
        "ax.plot(x,g)\n",
        "ax.plot(x,h)\n",
        "ax.plot(x,y)\n",
        "ax.grid()\n"
      ],
      "metadata": {
