      "source": [
        "### Funcion a trozos"
      ],
      "metadata": {
        "id": "daIkMURi-X4c"
      }
    },
    {
      "cell_type": "code",
      "source": [
        "def func_parts(x):\n",
        "  y=np.zeros(len(x))\n",
        "  for index,value in enumerate(x):\n",
        "    if value >= 0:\n",
        "      y[index] =1.0\n",
        "\n",
        "  return y\n",
        "\n",
        "func = func_parts(x)\n",
        "\n",
        "resolution = 500\n",
        "x = np.linspace(-10.0,10.0,num=resolution)\n",
        "\n",
        "\n",
        "fig, ax = plt.subplots()\n",
        "ax.plot(x,func)\n",
        "\n",
        "ax.grid()\n"
      ],
      "metadata": {
        "colab": {
          "base_uri": "https://localhost:8080/",
