      "source": [
        "#funcion lineal y= mx+b\n",
        "\n",
        "def func(m,x,b):\n",
        "  return m*x+b\n",
        "\n",
        "resolution = 100\n",
        "m = 1\n",
        "b = 15\n",
        "x = np.linspace(-10.0,10.0,num=resolution)\n",
        "y = func(m,x,b)\n",
        "\n",
        "fig, ax = plt.subplots()\n",
        "ax.plot(x,y)\n",
        "ax.grid()\n",
        "ax.axhline(y=0, color='r')\n",
        "ax.axvline(x=0,color='g')"
      ]
    },
