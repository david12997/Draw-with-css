        "def func_exp(base,x):\n",
        "\n",
        "  return base**x\n",
        "\n",
        "resolution = 500\n",
        "x = np.linspace(-10.0,10.0,num=resolution)\n",
        "\n",
        "base = 5\n",
        "\n",
        "expo = func_exp(base,x)\n",
        "\n",
        "\n",
        "fig, ax = plt.subplots()\n",
        "\n",
        "ax.plot(x,expo)\n",
        "\n",
        "ax.grid()\n",
