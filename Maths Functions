        "def func_cos(x):\n",
        "  return np.cos(x)\n",
        "\n",
        "def func_sen(x):\n",
        "  return np.sin(x)\n",
        "\n",
        "resolution = 90\n",
        "x = np.linspace(-10.0,10.0,num=resolution)\n",
        "\n",
        "coseno = func_cos(x)\n",
        "seno = func_sen(x)\n",
        "\n",
        "fig, ax = plt.subplots()\n",
        "\n",
        "ax.plot(x,seno)\n",
        "ax.plot(x,coseno)\n",
        "\n",
        "\n",
        "ax.grid()\n",
        "ax.axhline(y=0, color='r')\n",
        "ax.axvline(x=0,color='g')\n"
